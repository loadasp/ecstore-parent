<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.humau.ecstore.dao.UserDAO">
            
         <insert id="insertUser" parameterType="sysuser" keyProperty="id">
             insert into sysuser(  
         state,
         nickname) 
         values 
         (        
         #{state},
         #{nickname})
         </insert>
          
</mapper>
